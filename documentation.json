          },
          "400" : {
            "description" : "bad request"
          },
          "401" : {
            "description" : "unauthenticated"
          }
        }
      }
    },
    "/user" : {
      "get" : {
        "tags" : [ "users" ],
        "summary" : "current user",
        "description" : "returns current user",
        "parameters" : [ {
          "in" : "header",
          "name" : "Authorization",
          "schema" : {
            "type" : "string",
            "format" : "jwt"
          },
          "required" : true
        } ],
        "responses" : {
          "200" : {
            "description" : "current user",
            "content" : {
              "application/json" : {
                "schema" : {
                  "type" : "object",
                  "properties" : {
                    "email" : {
                      "type" : "string",
                      "example" : "mail@hirantha.xyz"
                    },
                    "family_name" : {
                      "type" : "string",
                      "example" : "name1"
                    },
                    "given_name" : {
                      "type" : "string",
                      "example" : "name2"
                    },
                    "photo" : {
                      "type" : "string",
                      "format" : "base64",
                      "example" : "base64img"
                    },
                    "gender" : {
                      "type" : "string",
                      "example" : "male",
                      "enum" : [ "male", "female" ]
                    },
                    "birthday" : {
                      "type" : "integer",
                      "example" : 0
                    },
                    "privilege" : {
                      "type" : "integer",
                      "example" : 10
                    }
                  }
                }
              }
            }
          },
          "401" : {
            "description" : "unauthenticated"
          }
        }
      }
    },
